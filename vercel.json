{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "crons": [
    {
      "path": "/api/save_daily_data",
      "schedule": "35 10 * * *"  
    },
    {
      "path": "/api/save_daily_data",
      "schedule": "30 11 * * *"  
    }
  ]
}